#!/Users/timkl/Documents/Git/txbxtx/venv/bin/python2.7
# EASY-INSTALL-ENTRY-SCRIPT: 'txbxtx','console_scripts','txbxtx'
__requires__ = 'txbxtx'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('txbxtx', 'console_scripts', 'txbxtx')()
    )
